import turtle
t = turtle.Turtle()
t.shape("turtle")
t.color("red")

# movement with pen

t.penup()
t.goto(100, 100)
t.pendown()

for i in range(3):
  t.fd(50)
  t.rt(120)
  
t.penup()
t.goto(0, 0)
t.pendown()

for i in range(4):
  t.fd(50)
  t.rt(90)

t.penup()
t.goto(-100, -100)
t.pendown()

for i in range(5):
  t.fd(50)
  t.rt(72)
  
